  "bugs": {
    "url": "https://github.com/typewriter-editor/json-patch/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.21",
    "@typewriter/delta": "^1.0.2",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "@typewriter/delta": "1.0.x",
    "@typewriter/document": "0.7.x"
  },
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/typewriter-editor/json-patch/",
  "keywords": [
    "immutable",
